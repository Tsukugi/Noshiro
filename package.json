{
  "name": "@atsu/noshiro",
  "version": "1.0.0",
  "type": "module",
  "description": "Input orchestration layer for TakaoEngine (TTY prompts and key handling)",
  "main": "dist/noshiro.cjs",
  "module": "dist/noshiro.js",
  "types": "dist/noshiro.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "test": "vitest run --passWithNoTests"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "input",
    "tty",
    "prompt",
    "typescript"
  ],
  "author": "Tsukugi <Tsukugi@proton.me>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.19.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
